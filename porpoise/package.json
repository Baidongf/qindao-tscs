{
  "name": "haizhi-poc",
  "version": "3.0.0-alpha.3",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "build": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "windowsBuild": "better-npm-run windowsBuild",
    "staticCompile": "better-npm-run staticCompile"
  },
  "betterScripts": {
    "staticCompile": {
      "command": "node bin/staticBuild",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "windowsBuild": {
      "command": "npm run clean && npm run staticCompile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cropperjs": "^1.0.0",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "d3": "^5.9.2",
    "d3-flextree": "^2.1.1",
    "debug": "^2.2.0",
    "echarts": "^4.2.0-rc.2",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.2",
    "jquery": "^3.2.1",
    "jquery-jcrop": "^0.9.13",
    "json-loader": "^0.5.4",
    "moment": "^2.18.1",
    "node-sass": "^4.13.1",
    "normalize.css": "^5.0.0",
    "orm": "^3.2.4",
    "pm2": "^2.5.0",
    "postcss-loader": "^1.1.0",
    "prop-types": "^15.5.6",
    "rc-pagination": "^1.10.0",
    "react": "15.6.1",
    "react-datepicker": "^0.46.0",
    "react-dom": "^15.0.0",
    "react-placeholder-support": "^1.0.3",
    "react-redux": "^5.0.1",
